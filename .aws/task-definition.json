{
    "containerDefinitions": [
        {
            "name": "manejo-api",
            "image": "768284818656.dkr.ecr.sa-east-1.amazonaws.com/manejo-api:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "manejo-api-5000-tcp",
                    "containerPort": 5000,
                    "hostPort": 0,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "family": "Production",
    "taskRoleArn": "arn:aws:iam::768284818656:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::768284818656:role/ecsTaskExecutionRole",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "330",
    "memory": "310",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
